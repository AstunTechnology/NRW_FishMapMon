<NamedLayer>
    <Name>{{info['name']}}</Name>
    <UserStyle>
        <Name>name</Name>
        <FeatureTypeStyle>
            <Rule>
                <Name>{{ _('Professional Hand Gather Extents') }}</Name>
                <LineSymbolizer>
                    <Stroke>
                        <SvgParameter name="stroke">#bf2717</SvgParameter>
                        <SvgParameter name="stroke-width">2px</SvgParameter>
                    </Stroke>
                </LineSymbolizer>
            </Rule>
        </FeatureTypeStyle>
    </UserStyle>
</NamedLayer>
